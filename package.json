{
  "name": "lrnstak-viz",
  "version": "1.0.0",
  "private": true,
  "main": "./src/main.ts",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 8080",
    "xdev-webpack": "webpack-dev-server --mode development",
    "xdev": "vue-cli-service serve --port 8080 --mode development",
    "xstart": "webpack --mode production",
    "start": "vite --host 0.0.0.0 --port 8080 --mode production",
    "build-tsc": "vue-tsc && vite build",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "run-p type-check vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
